{
  "name": "msc-dms-commons-angular",
  "version": "1.15.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "ng test --karmaConfig=./karma.conf.js",
    "build": "npm run cleanup && npm run build:lib && npm run link:i18n && npm pack ./dist/lib",
    "build:lib": "npm run build:shared && npm run build:core && npm run build:commons",
    "build:commons": "ng-packagr -p ./src/lib/ng-package.json",
    "build:core": "ng-packagr -p ./src/lib/core/ng-package.json && ncp ./dist/core ./node_modules/msc-dms-commons-angular/core",
    "build:shared": "ng-packagr -p ./src/lib/shared/ng-package.json && ncp ./dist/shared ./node_modules/msc-dms-commons-angular/shared",
    "link:i18n": "mkdirp ./dist/lib/common-i18n && ncp ./src/assets/i18n/ ./dist/lib/common-i18n",
    "cleanup": "rimraf ./dist && rimraf ./node_modules/msc-dms-commons-angular && mkdirp ./node_modules/msc-dms-commons-angular"
  },
  "private": true,
  "publishConfig": {
    "registry": "http://dms-zeus.cmc.ec.gc.ca/nexus/repository/npm-private/"
  },
  "dependencies": {
    "@ag-grid-community/angular": "~25.0.0",
    "@ag-grid-enterprise/all-modules": "~25.0.0",
    "@angular/animations": "11.0.2",
    "@angular/cdk": "^11.0.1",
    "@angular/common": "11.0.2",
    "@angular/core": "11.0.2",
    "@angular/forms": "11.0.2",
    "@angular/material": "^11.0.1",
    "@angular/platform-browser": "11.0.2",
    "@angular/platform-browser-dynamic": "11.0.2",
    "@angular/router": "11.0.2",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@ngrx/store": "^10.0.1",
    "@ngx-translate/core": "^11.0.1",
    "deepmerge": "2.1.1",
    "highcharts": "^7.2.2",
    "highcharts-angular": "^2.4.0",
    "ng-packagr": "^11.0.3",
    "ol": "^5.3.2",
    "rxjs": "~6.5.5",
    "rxjs-compat": "^6.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "11.0.2",
    "@angular/compiler": "11.0.2",
    "@angular/compiler-cli": "11.0.2",
    "@angular/language-service": "11.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^12.11.1",
    "@types/ol": "^4.6.5",
    "angular2-template-loader": "^0.6.2",
    "concurrently": "^3.4.0",
    "crypto-js": "^3.1.9-1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "primeicons": "^4.1.0",
    "primeng": "^11.1.0",
    "protractor": "~7.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^8.9.0",
    "tslint": "~6.1.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "4.0.5"
  }
