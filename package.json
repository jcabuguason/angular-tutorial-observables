    "ag-grid-angular": "^19.1.2",
    "ag-grid-community": "^19.1.4",
    "ag-grid-enterprise": "^19.1.4",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.4.0",
