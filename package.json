{
  "name": "msc-dms-commons-angular",
  "version": "1.15.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "ng test --karmaConfig=./karma.conf.js",
    "build": "npm run cleanup && npm run build:lib && npm run link:i18n && npm pack ./dist/lib",
    "build:lib": "npm run build:shared && npm run build:core && npm run build:commons",
    "build:commons": "ng-packagr -p ./src/lib/ng-package.json",
    "build:core": "ng-packagr -p ./src/lib/core/ng-package.json && ncp ./dist/core ./node_modules/msc-dms-commons-angular/core",
    "build:shared": "ng-packagr -p ./src/lib/shared/ng-package.json && ncp ./dist/shared ./node_modules/msc-dms-commons-angular/shared",
    "link:i18n": "mkdirp ./dist/lib/common-i18n && ncp ./src/assets/i18n/ ./dist/lib/common-i18n",
    "cleanup": "rimraf ./dist && rimraf ./node_modules/msc-dms-commons-angular && mkdirp ./node_modules/msc-dms-commons-angular"
  },
  "private": true,
  "publishConfig": {
    "registry": "http://dms-zeus.cmc.ec.gc.ca/nexus/repository/npm-private/"
  },
  "dependencies": {
    "@angular/animations": "9.1.2",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "9.1.2",
    "@angular/core": "9.1.2",
    "@angular/forms": "9.1.2",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "9.1.2",
    "@angular/platform-browser-dynamic": "9.1.2",
    "@angular/router": "9.1.2",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@ngrx/store": "^9.1.0",
    "@ngx-translate/core": "^11.0.1",
    "deepmerge": "2.1.1",
    "highcharts": "^7.0.0",
    "highcharts-angular": "^2.4.0",
    "ng-packagr": "^9.1.1",
    "ol": "^5.3.2",
    "rxjs": "~6.5.5",
    "rxjs-compat": "^6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "9.1.1",
    "@angular/compiler": "9.1.2",
    "@angular/compiler-cli": "9.1.2",
    "@angular/language-service": "9.1.2",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^12.11.1",
    "@types/ol": "^4.6.5",
    "ag-grid-angular": "~20.1.0",
    "ag-grid-community": "~20.1.0",
    "ag-grid-enterprise": "~20.1.0",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "^5.1.2",
    "concurrently": "^3.4.0",
    "crypto-js": "^3.1.9-1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.6",
    "protractor": "^5.4.0",
    "rimraf": "^2.6.2",
    "ts-node": "^8.9.0",
    "tslint": "~5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "3.8.3"
  }
