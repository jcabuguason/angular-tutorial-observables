    "primeicons": "^1.0.0-beta.9",
    "primeng": "^6.1.5",
    "protractor": "^5.4.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^3.2.0",
    "ts-node": "~3.2.0",
    "tsickle": "^0.24.1",
    "tslint": "~5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "~2.4.2",
    "webpack": "^3.8.1",
