{
  "name": "msc-dms-commons-angular",
  "version": "1.7.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "ng test --karmaConfig=./karma.conf.js",
    "build": "npm run cleanup && npm run build:lib && npm run link:i18n && npm pack ./dist/lib",
    "build:lib": "npm run build:shared && npm run build:core && npm run build:commons",
    "build:commons": "ng-packagr -p ./src/lib/ng-package.json",
    "build:core": "ng-packagr -p ./src/lib/core/ng-package.json && ncp ./dist/core ./node_modules/msc-dms-commons-angular/core",
    "build:shared": "ng-packagr -p ./src/lib/shared/ng-package.json && ncp ./dist/shared ./node_modules/msc-dms-commons-angular/shared",
    "link:i18n": "mkdirp ./dist/lib/common-i18n && ncp ./src/assets/i18n/ ./dist/lib/common-i18n",
    "cleanup": "rimraf ./dist && rimraf ./node_modules/msc-dms-commons-angular && mkdirp ./node_modules/msc-dms-commons-angular"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.7",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "7.2.7",
