    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "d3": "^4.9.1",
    "date-fns": "^1.29.0",
    "deepmerge": "^2.1.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "font-awesome": "^4.7.0",
    "helmet": "^3.9.0",
    "highcharts": "^7.0.0",
    "ldapjs": "^1.0.1",
    "rxjs": "~6.3.0",
    "rxjs-compat": "^6.4.0",
    "zone.js": "^0.8.29"
